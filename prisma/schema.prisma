generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  id        String    @id @default(uuid())
  publicKey String    @unique
  createdAt DateTime  @default(now())
  isFrozen  Boolean   @default(false)
  freeHours Float     @default(3)
  startDate DateTime?
}
